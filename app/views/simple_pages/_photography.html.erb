<div class="container-fluid">
  <div class="row">
    <div class="col-lg-10 col-sm-12 main-bar">
      <div class="container-fluid">
          <h1>Photography</h1>
        <div class="row">
          <% @photography_post.each do |post| %>
          <div class="col-lg-4 col-md-4 col-sm-12 theme-image">
            <%= image_tag(post.image_url, class: "img img-fluid") %>
          </div><!-- /col-lg-8 col-sm-12 -->
          <% end %>
        </div><!-- /row -->
      </div><!-- /container-fluid -->
    </div><!-- /main-bar -->
  </div><!-- /row -->
</div><!-- /posts-content -->
