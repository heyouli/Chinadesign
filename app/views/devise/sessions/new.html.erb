  <h1>Log in</h1>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="col-lg-6 col-sm-12 login-box">
      <div class="form-group">
    <%= f.label :email, class:"contact-label" %><br />
    <%= f.email_field :email, class: "form-control", autofocus: true, autocomplete: "email" %>
  </div>

    <div class="form-group">
      <%= f.label :password, class:"contact-label" %><br />
      <%= f.password_field :password, class: "form-control", autocomplete: "current-password" %>
    </div>

  <% if devise_mapping.rememberable? -%>
    <div class="checkbox-class">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me, class:"contact-label" %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in", id:"contact-button", class: "form_label btn btn-light message-button" %>
  </div>
<% end %>
<%= render "devise/shared/links" %>
</div>
